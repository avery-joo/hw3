<h1><%= @place["name"] %></h1>
<ul>
  <% @entries.each do |entry| %>
    <li>
      <h2><%= entry["title"] %></h2>
      <p><%= entry["occured_on"] %></p>
      <p><%= entry["description"] %></p>
    </li>
  <% end %>
</ul>

<p><a href="/entries/new?place_id=<%= @place["id"] %>">New Post</a></p>
<p><a href="/places">Places I've Been</a></p>